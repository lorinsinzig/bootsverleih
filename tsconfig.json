{
  "compilerOptions": {
    "noImplicitAny": true,
    "removeComments": true,
    "preserveConstEnums": true,
    "moduleResolution": "node",
    "sourceMap": true,
    "target": "es6",
    "lib": [
      "es2015",
      "es2015.iterable",
      "dom"
    ],
  },
  "exclude": [
    "test",
    "node_modules",
    "target",
    "project",
    "public",
    "lib",
    "conf"
  ]
}